@typeparam TItemLog
@using System.Diagnostics.CodeAnalysis

@Title

@foreach (var item in Items)
{
    if (Nakyvyys is not null)
    {
      @Nakyvyys(item)
    }
}

@code {

    [Parameter]
    public string Title { get; set; }

	[Parameter]
	public RenderFragment<TItemLog>? Nakyvyys { get; set; }

	[Parameter, AllowNull]
    public IReadOnlyList<TItemLog> Items { get; set; }
}